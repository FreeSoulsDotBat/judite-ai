{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"baseUrl": ".",
		"strict": true,
		"paths": {
			"@/*": ["./*"],
			"@components/*": ["components/*"],
			"@app/*": ["app/*"],
			"@constants/*": ["constants/*"],
			"@hooks/*": ["hooks/*"],
			"@config/*": ["config/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "index.js"]
}
